{
  "name": "cloverextension",
  "publisher": "maximmasterr",
  "displayName": "CloverExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cloverextension.topicVis",
    "onCommand:cloverextension.imageVis",
    "onCommand:cloverextension.serviceCaller",
    "onView:cloverTools"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "clover-tools",
          "title": "Clover tools",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "clover-tools": [
        {
          "id": "cloverTools",
          "name": "Clover tools",
          "icon": "media/logo.svg",
          "contextualTitle": "Clover tools"
        }
      ]
    },
    "commands": [
      {
        "command": "cloverextension.topicVis",
        "title": "Open topic visualisation"
      },
      {
        "command": "cloverextension.imageVis",
        "title": "Open image topic visualisation"
      },
      {
        "command": "cloverextension.serviceCaller",
        "title": "Open service caller"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --watch --mode development",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^4.2.2",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-typescript": "^0.33.0",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-promise-suspense": "^0.3.3",
    "roslib": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "xo": "^0.33.1"
  },
  "xo": {
    "extends": [
      "xo-react",
      "xo-typescript"
    ],
    "space": true,
    "semi": false,
    "prettier": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "import/no-unassigned-import": 0,
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "pascalCase": true
          }
        }
      ],
      "react/function-component-definition": [
        "error",
        {
          "namedComponents": "function-declaration"
        }
      ],
      "@typescript-eslint/no-loop-func": [
        0,
        {}
      ],
      "no-alert": 0,
      "react/prop-types": 0,
      "max-lines": [
        2,
        128
      ]
    },
    "ignore": [
      "src/ros/roslib.d.ts"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "repository": {
    "url": "https://github.com/FTL-team/cloverextension"
  }
}
