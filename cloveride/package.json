{
  "name": "cloveride",
  "publisher": "maximmasterr",
  "displayName": "CloverIDE",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cloveride.topicVis",
    "onCommand:cloveride.imageVis",
    "onCommand:cloveride.serviceCaller"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cloveride.topicVis",
        "title": "Open topic visualisation"
      },
      {
        "command": "cloveride.imageVis",
        "title": "Open image topic visualisation"
      },
      {
        "command": "cloveride.serviceCaller",
        "title": "Open service caller"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --watch --mode development",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/roslib": "^1.1.0",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^4.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "roslib": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "url": "https://github.com/FTL-team/cloverIDE"
  }
}
